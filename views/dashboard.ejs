        <div class="container">
            <div class="row">
              <div class="col-lg-12 wrap">
                <button type="submit" id="submitbtn" class="btn btn-success"/>Thumbnail Generator</button>
              </div>
            </div>
        </div>
      </form>
      <br>
      <form id="jsonInputGet" action="/v1/sc/jsoninput" method="GET">
